            "body": {
              "mode": "raw",
              "raw": "{\n  \"title\": \"Fresh Test Vegetables\",\n  \"description\": \"Test listing created via Postman for API testing\",\n  \"category\": \"produce\",\n  \"quantity\": 5,\n  \"unit\": \"kg\",\n  \"expiryDate\": \"{{$isoTimestamp}}\",\n  \"pickupWindow\": {\n    \"start\": \"{{$isoTimestamp}}\",\n    \"end\": \"{{$isoTimestamp}}\"\n  },\n  \"location\": {\n    \"latitude\": 40.7128,\n    \"longitude\": -74.0060,\n    \"address\": \"Test Location, NY\"\n  },\n  \"allergens\": [],\n  \"specialInstructions\": \"Test pickup instructions\"\n}"
            },
